:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
:vpi_module "v2009";
S_0x558cbb1ac340 .scope module, "alarmHex_tb" "alarmHex_tb" 2 3;
 .timescale -9 -12;
P_0x558cbb1ac4c0 .param/l "CLK_PERIOD" 0 2 6, +C4<00000000000000000000000000001010>;
v0x558cbb1f9370_0 .var "alarm_mode", 1 0;
v0x558cbb1f9450_0 .var "clk", 0 0;
v0x558cbb1f9520_0 .var "in_time", 15 0;
v0x558cbb1f9620_0 .net "ring", 0 0, v0x558cbb1f8f20_0;  1 drivers
v0x558cbb1f96f0_0 .var "rst", 0 0;
v0x558cbb1f97e0_0 .var "set_time", 15 0;
S_0x558cbb1e4a80 .scope module, "dut" "alarmHex" 2 17, 3 2 0, S_0x558cbb1ac340;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /INPUT 2 "alarm_mode"
    .port_info 3 /INPUT 16 "in_time"
    .port_info 4 /INPUT 16 "set_time"
    .port_info 5 /OUTPUT 1 "ring"
v0x558cbb1bf080_0 .net "alarm_mode", 1 0, v0x558cbb1f9370_0;  1 drivers
v0x558cbb1f8da0_0 .net "clk", 0 0, v0x558cbb1f9450_0;  1 drivers
v0x558cbb1f8e60_0 .net "in_time", 15 0, v0x558cbb1f9520_0;  1 drivers
v0x558cbb1f8f20_0 .var "ring", 0 0;
v0x558cbb1f8fe0_0 .net "rst", 0 0, v0x558cbb1f96f0_0;  1 drivers
v0x558cbb1f90f0_0 .net "set_time", 15 0, v0x558cbb1f97e0_0;  1 drivers
v0x558cbb1f91d0_0 .var "time_alarm", 15 0;
E_0x558cbb1e5280 .event posedge, v0x558cbb1f8fe0_0, v0x558cbb1f8da0_0;
    .scope S_0x558cbb1e4a80;
T_0 ;
    %wait E_0x558cbb1e5280;
    %load/vec4 v0x558cbb1f8fe0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x558cbb1f91d0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x558cbb1bf080_0;
    %cmpi/e 3, 0, 2;
    %flag_mov 8, 4;
    %jmp/0 T_0.2, 8;
    %load/vec4 v0x558cbb1f90f0_0;
    %jmp/1 T_0.3, 8;
T_0.2 ; End of true expr.
    %load/vec4 v0x558cbb1f91d0_0;
    %jmp/0 T_0.3, 8;
 ; End of false expr.
    %blend;
T_0.3;
    %assign/vec4 v0x558cbb1f91d0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x558cbb1e4a80;
T_1 ;
    %wait E_0x558cbb1e5280;
    %load/vec4 v0x558cbb1f8fe0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x558cbb1f8f20_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x558cbb1f91d0_0;
    %load/vec4 v0x558cbb1f8e60_0;
    %cmp/u;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %load/vec4 v0x558cbb1f8e60_0;
    %load/vec4 v0x558cbb1f91d0_0;
    %addi 1, 0, 16;
    %cmp/u;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0 T_1.2, 8;
    %pushi/vec4 1, 0, 2;
    %jmp/1 T_1.3, 8;
T_1.2 ; End of true expr.
    %pushi/vec4 0, 0, 2;
    %jmp/0 T_1.3, 8;
 ; End of false expr.
    %blend;
T_1.3;
    %pad/s 1;
    %assign/vec4 v0x558cbb1f8f20_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x558cbb1ac340;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x558cbb1f9450_0;
    %inv;
    %store/vec4 v0x558cbb1f9450_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x558cbb1ac340;
T_3 ;
    %vpi_call/w 2 31 "$dumpfile", "alarm.vcd" {0 0 0};
    %vpi_call/w 2 32 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x558cbb1ac340 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558cbb1f9450_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558cbb1f96f0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x558cbb1f9370_0, 0, 2;
    %pushi/vec4 4608, 0, 16;
    %store/vec4 v0x558cbb1f9520_0, 0, 16;
    %pushi/vec4 256, 0, 16;
    %store/vec4 v0x558cbb1f97e0_0, 0, 16;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558cbb1f96f0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x558cbb1f9370_0, 0, 2;
    %pushi/vec4 4617, 0, 16;
    %store/vec4 v0x558cbb1f97e0_0, 0, 16;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x558cbb1f9370_0, 0, 2;
    %pushi/vec4 9, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %load/vec4 v0x558cbb1f9520_0;
    %addi 1, 0, 16;
    %store/vec4 v0x558cbb1f9520_0, 0, 16;
    %delay 10000, 0;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %delay 100000, 0;
    %vpi_call/w 2 52 "$finish" {0 0 0};
    %end;
    .thread T_3;
